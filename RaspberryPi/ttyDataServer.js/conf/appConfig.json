{
  "app": {
    "webroot": "public",
    "dataFile": "dataDir/ttyData.log",
    "serialPort": {
      "device": "/dev/tty.MockSerial",
      "baudrate": 57600,
      "buffer": 128
    },
    "pumpIntervalSecs": 20,
    "pumpForceStopAfterSecs": 40,
    "pumpScheduleFile": "./conf/pumpSchedule.json"
  },
  "server": {
    "host": "localhost",
    "port": 7878
  }
}